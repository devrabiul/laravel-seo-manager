{
  "name": "devrabiul/laravel-seo-manager",
  "type": "library",
  "description": "Laravel SEO Manager is an SEO tool that improves SEO by adding recommended meta tags.",
  "keywords": [
    "seo",
    "tags",
    "social",
    "devrabiul",
    "laravel-seo-manager",
    "meta-tags",
    "search engine optimization",
    "meta description",
    "meta title",
    "open graph",
    "twitter cards",
    "structured data",
    "json-ld",
    "sitemap",
    "schema markup",
    "laravel seo",
    "seo manager",
    "laravel package",
    "seo tools"
  ],
  "homepage": "https://github.com/devrabiul/laravel-seo-manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Rabiul",
      "email": "devrabiul@gmail.com",
      "role": "Backend Developer"
    }
  ],
  "require": {
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "Devrabiul\\LaravelSeoManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devrabiul\\LaravelSeoManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devrabiul\\LaravelSeoManager\\SEOManagerServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan clear-compiled",
      "@php artisan config:clear",
      "@php artisan cache:clear"
    ]
  },
  "minimum-stability": "stable"
}
